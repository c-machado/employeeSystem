<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
		http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         id="WebApp_ID" version="3.1">
  <display-name>User Management Web Application</display-name>
  <servlet>
    <servlet-name>ServletEmployees</servlet-name>
    <servlet-class>ServletEmployees</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletEmployees</servlet-name>
    <url-pattern>/ServletEmployees</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletDepartment</servlet-name>
    <servlet-class>ServletDepartment</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletDepartment</servlet-name>
    <url-pattern>/ServletDepartment</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletRegulations</servlet-name>
    <servlet-class>ServletRegulations</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletRegulations</servlet-name>
    <url-pattern>/ServletRegulations</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ServletStatusReport</servlet-name>
    <servlet-class>ServletStatusReport</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletStatusReport</servlet-name>
    <url-pattern>/ServletStatusReport</url-pattern>
  </servlet-mapping>

</web-app>


